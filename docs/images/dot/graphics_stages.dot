# how graphics are processed
digraph{
  node[shape=rect]
   screencap editor redacted  
  node[shape=component]
    graphviz
    im_label
    im_scale
    im_border
    im_montage
  node [shape=ellipse]
  editor->{dot,sidebars}
  sidebars->im_label->sidebars
  screencap->{raw,safe}
  dot->graphviz->safe
  {sidebars,im_border}->im_montage
  raw->redacted->safe
  {safe}->im_scale->scaled->im_border
  {im_border,im_montage}->tgt


}