    },
    {
      "name": "oledump args",
      "type": "python",
      "request": "launch",
      "program": "util/oledump.py",
      "console": "integratedTerminal",
      "args": ["-r","-v","--vbadecompress", "./tmp/vbaProject/VBA/Module1"],
      "justMyCode": true
    },
    {
      "name": "sort_vba args",
      "type": "python",
      "request": "launch",
      "program": "dance/util/sort_vba.py",
      "console": "integratedTerminal",
      "args": ["vba/fcast_vba.txt","vba/sorted_vba.txt","-d","docs/source/vba_index.md"],
      "justMyCode": true
    },
    {
      "name": "create_wb args",
      "type": "python",
      "request": "launch",
      "program": "dance/util/create_wb.py",
      "console": "integratedTerminal",
      "args": ["data/test_wb.zip"],
      "justMyCode": true
    }
  ]
